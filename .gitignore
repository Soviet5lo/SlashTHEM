# /doc/
/doc/Makefile

# /include/
/include/autoconf.h
/include/autoconf_paths.h
/include/date.h
/include/dgn_comp.h
/include/lev_comp.h
/include/onames.h
/include/pm.h
/include/vis_tab.h
/include/win32api.h

# /src/
/src/*.o
/src/nhwin.a
/src/slashem
/src/Makefile
/src/Sysunix
/src/config.h-t
/src/hack.h-t
/src/vis_tab.c
/src/monstr.c
/src/tile.c

# /util/
/util/*.o
/util/Makefile
/util/dgn_comp
/util/dgn_lex.c
/util/dgn_yacc.c
/util/dlb
